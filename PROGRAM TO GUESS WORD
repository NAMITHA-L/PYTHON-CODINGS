str1 = 'chennai'
str2 = 'madurai'
str3 = 'vellore'
str4 = 'kanyakumari'
str5 = 'coimbatore'


lst = []
lst1 = []


n1 = input("Enter 1st character: ")
m1 = input("Enter 2nd character: ")


if n1 in str1:
    lst.append(str1)
if n1 in str2:
    lst.append(str2)
if n1 in str3:
    lst.append(str3)
if n1 in str4:
    lst.append(str4)
if n1 in str5:
    lst.append(str5)


for item in lst:
    if m1 in item:
        lst1.append(item)


if len(lst1) > 1:
    c = input("Enter another character: ")
    for item in lst:
        if c in item:
            print(item)
else:
    for item in lst1:
        print(item)
